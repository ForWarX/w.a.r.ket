<script type="text/javascript">
    $(function(){
        $("#flyer_img").click(function(){
            var width = $(this).width();
            if(width==230)
            {
                $(this).width(800);
                $(this).height(800);
            }
            else
            {
                $(this).width(230);
                $(this).height(230);
            }
        });
    });
</script>
